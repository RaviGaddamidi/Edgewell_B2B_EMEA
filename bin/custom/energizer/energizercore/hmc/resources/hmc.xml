<configuration xmlns="hybris.de/schemas/hmc/generic"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="hybris.de/schemas/hmc/generic hmc.xsd">


	<explorertree>
		<group name="EnergizerGroup" description="group.energizer.description">
			<typeref type="EnergizerB2BUnit" description="energizer.b2bunit.description" />
			<typeref type="EnergizerProduct" description="energizer.product.description" />
			<typeref type="EnergizerCMIR" description="energizer.cmir.description" />
			<typeref type="EnergizerProductConversionFactor" description="energizer.productconversionfactor.description" />
			<typeref type="EnergizerSalesAreaUOM" description="energizer.salesareauom.description" />
			<typeref type="EnergizerB2BUnitLeadTime" description="energizer.b2bunitleadtime.description" />
			<typeref type="EnergizerB2BCustomer" description="energizer.b2bcustomer.description" />
			<typeref type="EnergizerPriceRow" description="energizer.pricerow.description" />
			<typeref type="EnergizerAddress" description="energizer.address.description" />
			<typeref type="MetricUnit" description="energizer.metricunit.description" />
			<typeref type="EnergizerShippingPoint" description="energizer.shippingpoint.description" />
			<typeref type="EnergizerRegion" description="energizer.region.description" />
			<typeref type="EnergizerRegionGenericProducts" description="energizer.regiongenericproducts.description" />
		</group>
		
	</explorertree>

	<type name="Order" mode="append">
		<organizer>
			<search mode="append">
				<condition attribute="versionID" operator="is null" />
			</search>
		</organizer>
	</type>

</configuration>