			<!-- <itemtype code="EnergizerOrder" extends="Order" autocreate="true" -->
			<!-- Surendra : As discussed with Sriram and Kaushik changing item definition - renaming from energizer order to abstract order 
				and movine few attribute to order level -->

			<itemtype code="AbstractOrder" autocreate="false" generate="false">

				<description>Energizer order with specific SAP address ID
				</description>
				OrderNumber - OOTB
				Customer - Would be an energizer b2b customer
				Currency for all prices will be refered from the OOTB order
				level
				currency
				Freight charges - OOTB delivery cost
				Taxes - OOTB
				Sold to - OOTB billing address - this uses EnergizerAddress Model
				Ship to -
				OOTB shipping address - this uses EnergizerAddress Model
				Order Total - OOTB
				Order Currency - OOTB
				Purchase order number - OOTB 

				<attributes>
					<attribute qualifier="b2bUnit" type="EnergizerB2BUnit">
						<description>Energizer B2B Unit  - This will be used for Listing all orders for a B2B unit</description>
						<modifiers />
						<persistence type="property" />
					</attribute>
					<attribute qualifier="orderType" type="java.lang.String">
						<description>export/ domestic</description>
						<modifiers />
						<persistence type="property" />
					</attribute>
					<attribute qualifier="shippingPointId" type="java.lang.String">
						<description>ID of the distribution center</description>
						<modifiers />
						<persistence type="property" />
					</attribute>
